{
  "expo": {
    "name": "Popcorn",
    "slug": "Popcorn",
    "version": "38.0.8",
    "orientation": "portrait",
    "icon": "./app/assets/favicon.png",
    "splash": {
      "image": "./app/assets/p_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f4f4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/p_logo.png"
    },
    "description": "Can't ever agree on what to watch? Popcorn will let you and your movie-watching-partner match with a movie that you'll both love!\n\nWant to try without creating an account and adding a friend? Log in as Cody with email: cody@email.com and password: 12345",
    "githubUrl": "https://github.com/rileybutterfield/popcorn"
  }
}
